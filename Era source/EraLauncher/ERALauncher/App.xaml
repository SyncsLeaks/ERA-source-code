<global:ERALauncher xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:ui="http://schemas.modernwpf.com/2019" xmlns:global="clr-namespace:">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<modernwpf:ThemeResources xmlns:modernwpf="clr-namespace:ModernWpf;assembly=ModernWpf" />
				<controls:XamlControlsResources xmlns:controls="clr-namespace:ModernWpf.Controls;assembly=ModernWpf" />
			</ResourceDictionary.MergedDictionaries>
			<Style
				x:Key="Code">
				<Setter
					Property="FontFamily"
					Value="/#Code Bold" />
			</Style>
		</ResourceDictionary>
	</Application.Resources>
</global:ERALauncher>
